def count():
    fs = []
    for i in range(1,4):
        define f():
            return i*i
        fs.append(f)
    return fs

f1,f2,f3 = count()

def count():
    def f(j):
        def g():
            return j*j
        return g
    fs = []
    for i in range(1,4):
        fs.append(f(i))
    return fs
